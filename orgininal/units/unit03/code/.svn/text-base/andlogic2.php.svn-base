<html>
 <head>
  <title>Logical Word Operators</title>
 </head>
 <body bgcolor="330066" text="white">
  <font face="arial" size="+1">
  <h3>Dealing with Precedence</h3>
  <hr>
  <?php
    $x = 5;
    $y = 6;
    $z = 0;
    
    $result = $x && $y && $z;
    echo "$x && $y && $z <em>yields</em> ".
         (int)$result .".\n<br />";

    $result = $x and $y and $z;
    echo "$x and $y and $z <em>yields</em> " .
         (int)$result .".\n<br />";

    $result = ($x and $y and $z);
    echo "($x and $y and $z) <em>yields</em> " .
         (int)$result .".\n<br />";
  ?>
  <hr>
 </body>
</html>

